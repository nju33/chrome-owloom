{
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "1.0.1",
  "default_locale": "ja",
  "permissions": [
    "bookmarks",
    "tabs",
    "storage"
  ],
  "icons": {
    "16": "icons/icon_16x16.png",
    "48": "icons/icon_48x48.png",
    "128": "icons/icon_128x128.png"
  },
  "background": {
    "scripts": ["scripts/background.bundle.js"],
    "persistent": true
  },
  "commands": {
    "open-owloom": {
      "global": true,
      "suggested_key": {
        "default": "Alt+B",
        "mac": "Alt+B"
      },
      "description": "Open Lwloom window"
    }
  },
  "browser_action": {
    "default_icon": "icons/icon_48x48.png",
    "default_title": "Owloom"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-Lqho6QpJNxvyP3MbeLoIfSju4S5V9hFCJTIAy+P1XL4='; object-src 'self'"
}
