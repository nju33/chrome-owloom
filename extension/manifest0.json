{
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "1.0.0.0",
  "default_locale": "ja",
  "permissions": [
    "bookmarks",
    "tabs",
    "storage"
  ],
  "icons": {
    "16": "icons/icon_16x16.png",
    "48": "icons/icon_48x48.png",
    "128": "icons/icon_128x128.png"
  },
  "background": {
    "scripts": ["scripts/background.bundle.js"],
    "persistent": true
  },
  "commands": {
    "open-owloom": {
      "global": true,
      "suggested_key": {
        "default": "Alt+B",
        "mac": "Alt+B"
      },
      "description": "Open Lwloom window"
    }
  },
  "sandbox": {
    "pages": [
      "app/index.html",
      "app/bookmark/index.html"
    ],
    "content_security_policy":
      "sandbox allow-scripts allow-forms allow-popups allow-modals; script-src 'self' 'unsafe-inline' 'unsafe-eval' 'sha256-lZoc+IinsSIcycLoedjCAXcQ31fKG6yKyUviz8jRQ7U='; child-src 'self';"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-lZoc+IinsSIcycLoedjCAXcQ31fKG6yKyUviz8jRQ7U='; object-src 'self'"
}
